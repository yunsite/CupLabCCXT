{
    "manifest_version": 2,
    "name": "CupLab DCE",
    "description": "CupLab Digital Currency Exchange JavaScript API with Chrome Extension",
    "version": "0.0.1",
	"background": {
        "scripts": ["js/background.js"]
	},
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_title": "CupLab DCE"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [       
        "tabs",            
        "webRequestBlocking",
        "webRequest",
        "browsingData",
        "storage",
        "unlimitedStorage",
        "https://api.dobitrade.com/*"
    ],
    "icons" : {
        "128": "images/icon.png"
    },
    "options_page":"index.html"
}
